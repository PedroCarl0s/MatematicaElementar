    <style name="AttribouterTheme.Dialog" parent="ThemeOverlay.AppCompat.Dialog"/>
    <style name="AttribouterTheme.Dialog.Fullscreen" parent="AttribouterTheme">
        <item name="android:windowAnimationStyle">@style/AttribouterTheme.SlideAnimation</item>
    </style>
    <style name="AttribouterTheme.SlideAnimation">
        <item name="android:windowEnterAnimation">@anim/attribouter_slide_in</item>
        <item name="android:windowExitAnimation">@anim/attribouter_slide_out</item>
    </style>
    <declare-styleable name="AttribouterTheme">
        <attr format="reference" name="personDialogTheme"/>
        <attr format="reference" name="overflowDialogTheme"/>
        <attr format="reference" name="licenseDialogTheme"/>
    </declare-styleable>
